<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--配置数据源,交给spring去做-->
    <typeAliases>
        <package name="com.pl.pojo"/>
    </typeAliases>
    <mappers>
        <mapper resource="com/pl/dao/AdminMapper.xml"></mapper>
        <mapper resource="com/pl/dao/HouseMapper.xml"></mapper>
        <mapper resource="com/pl/dao/OrderMapper.xml"></mapper>
        <mapper resource="com/pl/dao/UserMapper.xml"></mapper>
    </mappers>
</configuration>